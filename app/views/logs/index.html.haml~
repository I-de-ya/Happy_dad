%ul
	- @logs.each do |log|
		%li
			У прибора
			%b= log.device.title
			значение атрибута
			%b= log.attr
			- if log.old_value == nil
				было установлено равным					
				%b= log.new_value
			- else
				было изменено с 		
				%b= log.old_value
				на
		= log.created_at.to_s(:db)
		%hr
	
	

